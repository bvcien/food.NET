<div class="row  custom-style">

    @foreach (var hoaquas in Model)
    {
        <div class="col-6 col-md-4 col-lg-3 product_item container ">
            <div class="card ">
                <!-- Hình ảnh -->
                <img src="~/@hoaquas.ImageUrl" class="card-img-top" alt="Image of @hoaquas.Title"
                    style="height: 200px; object-fit: cover;">

                <div class="card-body ">
                    <!-- Tiêu đề -->
                    <h5 class="card-title">@hoaquas.Title</h5>

                    <!-- Thể loại -->
                    <p class="card-text">
                        <strong>Ngày nhập:</strong> @hoaquas.Genre
                    </p>

                    <!-- Giá -->
                    <p class="card-text">
                        <strong>Giá:</strong> @hoaquas.Price.ToString("#,0") VND
                    </p>
                </div>

                <div class="card-footer">
                    <!-- Thêm các nút hành động nếu cần -->

                    <a asp-controller="Hoaqua" asp-action="Chitiet" asp-route-id="@hoaquas.Id" class="btn btn-success">Mua
                        Ngay</a>
                </div>
            </div>
        </div>
    }
</div>
<style>
    /* Đảm bảo chữ không bị gạch ngang */
    .no-line-through {
        text-decoration: none !important;
    }

    /* Định nghĩa bổ sung nếu cần */
    .custom-style {
        font-family: 'Quicksand', sans-serif;
        color: #333;
    }
</style>
